# Sui Network Configuration
NEXT_PUBLIC_SUI_RPC_URL=https://fullnode.testnet.sui.io

# zkLogin Configuration
NEXT_PUBLIC_PROVER_URL=https://prover-dev.mystenlabs.com/v1

# Google OAuth Configuration
# Create credentials at: https://console.cloud.google.com/apis/credentials
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# OAuth Redirect URL (must match your Google OAuth config)
# For local development: http://localhost:3000/auth/callback
# For production: https://your-domain.com/auth/callback
NEXT_PUBLIC_REDIRECT_URL=http://localhost:3000/auth/callback

# ============== Demo/Hackathon Mode ==============
# Set to 'true' to enable demo mode with simulated data
# This allows the app to work without real TEE/Seal infrastructure
NEXT_PUBLIC_DEMO_MODE=true

# ============== Contract Addresses (Testnet) ==============
NEXT_PUBLIC_INTENT_REGISTRY_PACKAGE=0xe29fd9c9698d416c6f4327fe83e06dad7116302f6efabef96b94d9ab86442656
NEXT_PUBLIC_SEAL_POLICY_PACKAGE=0xdacdece21b4b19fe7b5631a9594056fc01132a11d5dc75498a4f4c4a641b9c37
NEXT_PUBLIC_INTENT_REGISTRY_OBJECT=0x4a7e401d3cf98cb1e22b6443c5acff556ffc1e78dd5319dca8bdfc73edbc053c

# DeepBook V3 (Testnet)
NEXT_PUBLIC_DEEPBOOK_PACKAGE=0x337f4f4f6567fcd778d5454f27c16c70e2f274cc6377ea6249ddf491482ef497

# ============== NEAR Intents AI Agent ==============
# NEAR Mainnet Account for auto-executing swaps
# The agent uses this account to send deposits to the 1-Click API
SENDER_NEAR_ACCOUNT=your-account.near
SENDER_PRIVATE_KEY=ed25519:your-private-key-here

# Optional: JSON Web Token for 0-fee swaps via 1-Click API
# Without it, swaps work but incur a 0.1% fee
# Request one here: https://docs.google.com/forms/d/e/1FAIpQLSdrSrqSkKOMb_a8XhwF0f7N5xZ0Y5CYgyzxiAuoC2g4a2N68g/viewform
NEAR_INTENTS_JWT=your-near-intents-jwt

# ============== Telegram Bot Configuration ==============
# Get from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
